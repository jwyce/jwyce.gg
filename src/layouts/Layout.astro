---
import '../assets/global.css';
export interface Props {
	title: string;
}

const { title } = Astro.props as Props;
---

<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width" />
		<meta property="og:title" content="Jared Wyce" />
		<meta
			property="og:description"
			content="Personal Website. A collection of technology I use and projects I've worked on."
		/>
		<link rel="icon" type="image/x-icon" href="/favicon.ico" />
		<title>{title}</title>
	</head>

	<body>
		<slot />
	</body>
</html>
